        "laravel-mix": "^1.0",
        "lodash": "^4.17.4",
        "vue": "^2.5.7"
    },
    "dependencies": {
        "laravel-vue-pagination": "^1.0.6",
        "vue-axios": "^2.0.2",
        "vue-ckeditor2": "^1.18.0",
        "vue-multiselect": "^2.0.8",
        "vue-router": "^3.0.1",
        "vue-select": "^2.3.1",
        "vue-simple-spinner": "^1.2.8",
        "vue-swal": "0.0.6",
        "vue2-selectize": "^1.1.4"
    }
